{
  "name": "dosomething/northstar",
  "description": "The DoSomething.org user & activity API.",
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dosomething/laravel-mongodb"
    }
  ],
  "require": {
    "php": "~7.2.0",
    "aws/aws-sdk-php": "^3.46",
    "barryvdh/laravel-cors": "^0.11.2",
    "dfurnes/environmentalist": "0.0.2",
    "dosomething/gateway": "^1.14.3",
    "dosomething/stathat": "^2.0.0",
    "fideloper/proxy": "^3.3",
    "giggsey/libphonenumber-for-php": "^7.0",
    "gree/jose": "^2.2",
    "guzzlehttp/guzzle": "^6.3.0",
    "jenssegers/mongodb": "dev-3.3.1-patched-reset",
    "laravel/browser-kit-testing": "1.*",
    "laravel/framework": "5.5.*",
    "laravel/socialite": "^3.0.0",
    "laravel/tinker": "^1.0",
    "league/csv": "^9.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/fractal": "0.13.*",
    "league/iso3166": "^1.0",
    "league/oauth2-server": "^7.0.0",
    "mongodb/mongodb": "~1.2.0",
    "predis/predis": "^1.1",
    "seatgeek/sixpack-php": "^2.1",
    "symfony/psr-http-message-bridge": "^1.0.0",
    "zendframework/zend-diactoros": "^1.3"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "itsgoingd/clockwork": "^2.0",
    "mockery/mockery": "^0.9.4",
    "phpspec/phpspec": "~2.1",
    "phpunit/phpunit": "~5.7",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "Northstar\\": "app/"
    }
  },
  "extra": {
    "laravel": {
        "dont-discover": [
            "jenssegers/mongodb"
        ]
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/BrowserKitTestCase.php",
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "warmup": [
      "@php artisan config:cache"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}

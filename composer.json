{
  "name": "dosomething/northstar",
  "description": "The DoSomething.org user & activity API.",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "~7.4.0",
    "ext-bcmath": "*",
    "ext-exif": "*",
    "ext-gd": "*",
    "ext-gettext": "*",
    "ext-newrelic": "*",
    "ext-redis": "*",
    "algolia/algoliasearch-client-php": "^2.6",
    "aws/aws-sdk-php-laravel": "^3.1",
    "aws/aws-sdk-php": "^3.134",
    "barryvdh/laravel-cors": "^0.11.2",
    "dfurnes/environmentalist": "0.0.7",
    "doctrine/dbal": "~2.5.13",
    "dosomething/gateway": "^3.0",
    "endroid/qr-code": "^3.6",
    "fideloper/proxy": "^4.0",
    "giggsey/libphonenumber-for-php": "^8.12",
    "gree/jose": "^2.2",
    "guzzlehttp/guzzle": "^6.3.0",
    "hashids/hashids": "^4.0",
    "intervention/image": "^2.3",
    "jenssegers/mongodb": "3.6.*",
    "laminas/laminas-diactoros": "^2.2",
    "langleyfoxall/laravel-nist-password-rules": "^4.2",
    "laravel/browser-kit-testing": "5.*",
    "laravel/framework": "^6.20.11",
    "laravel/scout": "^7.2.1",
    "laravel/slack-notification-channel": "^2.1",
    "laravel/socialite": "^4.0.0",
    "lcobucci/jwt": "~3.3.2",
    "league/csv": "^9.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/flysystem-memory": "^1.0",
    "league/fractal": "0.18.*",
    "league/iso3166": "^2.1",
    "league/glide-laravel": "^1.0",
    "league/oauth2-server": "~8.1.1",
    "mmucklo/email-parse": "^2.0",
    "mongodb/mongodb": "~1.4.0",
    "seatgeek/sixpack-php": "^2.1",
    "softonic/graphql-client": "^1.2",
    "sokil/php-isocodes": "^3.0",
    "spomky-labs/otphp": "^10.0",
    "symfony/psr-http-message-bridge": "^1.0.0"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.0",
    "facade/ignition": "^1.4",
    "fzaninotto/faker": "^1.9.1",
    "itsgoingd/clockwork": "^3.0",
    "laravel/tinker": "^2.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "weerd/php-style": "^1.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "jenssegers/mongodb"
      ]
    }
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/BrowserKitTestCase.php",
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "warmup": [
      "@php artisan config:cache"
    ]
  }
}

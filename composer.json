{
  "name": "dosomething/northstar",
  "description": "The DoSomething.org user & activity API.",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "~7.3.0",
    "ext-newrelic": "*",
    "ext-redis": "*",
    "aws/aws-sdk-php": "^3.134",
    "barryvdh/laravel-cors": "^0.11.2",
    "dfurnes/environmentalist": "0.0.6",
    "dosomething/gateway": "^3.0",
    "endroid/qr-code": "^3.6",
    "fideloper/proxy": "^4.0",
    "giggsey/libphonenumber-for-php": "^8.12",
    "gree/jose": "^2.2",
    "guzzlehttp/guzzle": "^6.3.0",
    "jenssegers/mongodb": "3.6.*",
    "laminas/laminas-diactoros": "^2.2",
    "laravel/browser-kit-testing": "5.*",
    "laravel/framework": "^6.2",
    "laravel/helpers": "^1.2",
    "laravel/socialite": "^4.0.0",
    "league/csv": "^9.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/fractal": "0.18.*",
    "league/iso3166": "^2.1",
    "league/oauth2-server": "^8.0.0",
    "mmucklo/email-parse": "^2.0",
    "mongodb/mongodb": "~1.4.0",
    "seatgeek/sixpack-php": "^2.1",
    "softonic/graphql-client": "^1.2",
    "spomky-labs/otphp": "^10.0",
    "symfony/psr-http-message-bridge": "^1.0.0"
  },
  "require-dev": {
    "facade/ignition": "^1.4",
    "fzaninotto/faker": "^1.9.1",
    "itsgoingd/clockwork": "^3.0",
    "laravel/tinker": "^2.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "Northstar\\": "app/"
    }
  },
  "extra": {
    "laravel": {
        "dont-discover": [
            "jenssegers/mongodb"
        ]
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/BrowserKitTestCase.php",
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "warmup": [
      "@php artisan config:cache"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}

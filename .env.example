APP_URL=http://northstar.test
APP_ENV=local
APP_DEBUG=true

# Generated by running `php artisan northstar:setup`
# or `php artisan key:generate` scripts:
APP_KEY=

# Token encryption key, generated by `php artisan northstar:setup`
# or `vendor/bin/generate-defuse-key` scripts:
APP_AUTH_KEY=

# Feature Flags
DS_ENABLE_BLINK=false
DS_ENABLE_RATE_LIMITING=false
DS_ENABLE_DELETE_APIS=false
DS_OPTIONAL_FIELDS=false
DS_BADGES_TEST=false
DS_REFER_FRIENDS_SCHOLARSHIP_TEST=false
DS_CONTENTFUL_IDS_FOR_CAMPAIGNS_WITH_NO_BADGES=

# Drivers:
MAIL_DRIVER=log
STORAGE_DRIVER=local
QUEUE_DRIVER=sync

# Mail:
MAIL_ADDRESS=noreply@dosomething.org
MAIL_NAME=DoSomething.org

# StatHat:
STATHAT_EZ_KEY=puppet-sloth@example.com
STATHAT_APP_NAME=northstar-dev

# Facebook authentication:
FACEBOOK_REDIRECT_URL=https://northstar.dev/facebook/verify
FACEBOOK_APP_SECRET=FacebookSecret
FACEBOOK_APP_ID=FacebookAppId

# Google authentication:
GOOGLE_CLIENT_ID=GoogleClientId
GOOGLE_CLIENT_SECRET=GoogleSecret
GOOGLE_REDIRECT_URL=https://northstar.dev/google/verify

# Other DoSomething.org services:
PHOENIX_URL=https://dev.dosomething.org
ROGUE_URL=https://activity-dev.dosomething.org
GAMBIT_URL=https://gambit-conversations-staging.herokuapp.com
GAMBIT_USERNAME=
GAMBIT_PASSWORD=

# Customer.io:
CUSTOMER_IO_URL=https://track.customer.io/api/v1/
CUSTOMER_IO_USERNAME=siteName
CUSTOMER_IO_PASSWORD=apiKey

# Sixpack:
SIXPACK_ENABLED=false
SIXPACK_BASE_URL=null
SIXPACK_COOKIE_PREFIX=null
SIXPACK_TIMEOUT=null

# Google Analytics:
GOOGLE_TAG_MANAGER_ID=

# Snowplow Analytics
SNOWPLOW_URL= # without https://

#GraphQL
GRAPHQL_URL=

